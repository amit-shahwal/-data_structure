            {
                printf("kindly enter your cell number\n");
            scanf("%ld",&ph);
            printf("we will shortly send you the link on %ld cell no\n\n",ph);
        }else
        {
            exit(0);
        }
        printf("wana get more result press 1 and to exit press 2\n");
        scanf("%d",&g);

        if(g==1)

            {goto amit;}
        else{exit(0);}}

}
return 0;
}

